  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "allowJs": true,
    "checkJs": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "../dist/electron"
  },
  "include": ["./**/*.js", "./**/*.ts"],
  "exclude": ["node_modules"]
} 