{
  "name": "flujo",
  "version": "0.1.2",
  "private": true,
  "main": "electron/main.js",
  "scripts": {
    "dev": "next dev -p 4200",
    "turbo": "next dev --turbopack -p 4200",
    "build": "next build",
    "start": "next start -p 4200",
    "lint": "next lint",
    "electron-dev": "concurrently \"npm run dev\" \"wait-on http://localhost:4200 && electron .\"",
    "electron-build": "next build && electron-builder",
    "electron-start": "electron .",
    "electron-pack": "npm run build && electron-builder --dir",
    "electron-dist": "npm run build && electron-builder",
    "electron-dist-all": "npm run build && electron-builder -mwl",
    "postinstall": "node scripts/custom-install.js",
    "postinstall:electron": "electron-builder install-app-deps",
    "install:server": "HEADLESS_SERVER=true npm install --no-optional",
    "headless": "electron . --headless",
    "headless-docs": "electron . --headless --docs",
    "headless-timeout": "electron . --headless --timeout=60",
    "headless-optimize": "electron . --headless --expose-gc",
    "headless-shutdown": "node scripts/shutdown-headless.js",
    "open-docs": "node scripts/open-docs.js",
    "open-docs:standard": "node scripts/open-docs.js --standard",
    "open-docs:api": "node scripts/open-docs.js --api",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build:server": "NODE_ENV=production SERVER_BUILD=true next build",
    "deploy:server": "node scripts/server-deploy.js",
    "start:server": "NODE_ENV=production SERVER_BUILD=true npm run start",
    "start:server:network": "NODE_ENV=production SERVER_BUILD=true FLUJO_NETWORK_MODE=1 npm run start",
    
    "headless-server": "node scripts/headless-server.js",
    "headless-server:network": "node scripts/headless-server.js --network",
    "headless-server:dev": "NODE_ENV=development node scripts/headless-server.js",
    "build:headless": "HEADLESS_SERVER=true npm run build",
    "build:ci": "NODE_ENV=production HEADLESS_SERVER=true SERVER_BUILD=true next build",
    "deploy:headless": "HEADLESS_SERVER=true npm run install:server && npm run build:headless",
    "install:headless": "HEADLESS_SERVER=true FORCE_SERVER_MODE=true npm install --production=false --no-optional",
    
    "setup:swap": "bash scripts/create-swap.sh",
    "build:low-memory": "node scripts/build-low-memory.js",
    "deploy:low-memory": "npm run install:headless && npm run build:low-memory",
    "deploy:minimal": "npm run setup:swap && npm run deploy:low-memory && npm run headless-server:network",
    "deploy:1gb": "bash scripts/ultra-low-memory-deploy.sh",
    "deploy:from-artifacts": "bash scripts/deploy-server-from-artifact.sh"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@isomorphic-git/lightning-fs": "^4.6.0",
    "@modelcontextprotocol/sdk": "^1.7.0",
    "@mui/icons-material": "^6.4.8",
    "@mui/material": "^6.4.8",
    "@types/crypto-js": "^4.2.2",
    "@types/swagger-ui": "^3.52.4",
    "@types/swagger-ui-react": "^5.18.0",
    "@types/uuid": "^10.0.0",
    "@uiw/react-md-editor": "^4.0.5",
    "@xenova/transformers": "^2.17.2",
    "@xyflow/react": "^12.4.4",
    "crypto-js": "^4.2.0",
    "install": "^0.13.0",
    "isomorphic-git": "^1.30.1",
    "jszip": "^3.10.1",
    "lodash": "^4.17.21",
    "minimist": "^1.2.8",
    "next": "15.2.4",
    "node-fetch": "^3.3.2",
    "openai": "^4.89.0",
    "prismjs": "^1.30.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "^15.6.1",
    "remark-gfm": "^4.0.1",
    "simple-git": "^3.27.0",
    "slate": "^0.112.0",
    "slate-dom": "^0.112.2",
    "slate-history": "^0.110.3",
    "slate-react": "^0.112.1",
    "swagger-ui-react": "^5.20.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-typescript": "^7.27.0",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-typescript": "^7.27.0",
    "@eslint/eslintrc": "^3",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.16",
    "@types/node": "^22",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/ws": "^8.18.0",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "buffer": "^6.0.3",
    "concurrently": "^9.1.2",
    "eslint": "^9.23.0",
    "eslint-config-next": "15.2.4",
    "eslint-import-resolver-typescript": "^4.2.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "jest": "^29.7.0",
    "node-loader": "^2.1.0",
    "postcss": "^8",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^10.1.5",
    "process": "^0.11.10",
    "ts-jest": "^29.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "wait-on": "^8.0.3",
    "webpack": "^5.98.0",
    "ws": "^8.18.1"
  },
  "optionalDependencies": {
    "electron": "^35.1.0",
    "electron-builder": "^25.1.8",
    "electron-is-dev": "^3.0.1"
  },
  "overrides": {
    "swagger-ui-react": {
      "react": "$react",
      "react-dom": "$react-dom"
    }
  }
}
